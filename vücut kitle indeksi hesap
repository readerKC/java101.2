import java.util.Scanner;

public class Main {
    public static void main(String[] argd) {
        Scanner input = new Scanner(System.in);
        Float a,b;

        System.out.print("Boy değerinizi giriniz(m): ");
        a = input.nextFloat();

        System.out.print("Kilo değerinizi giriniz(kg): ");
        b = input.nextFloat();

        System.out.println("Vücut kitle indeksiniz: " + (b/(a*a)));
    }


}
